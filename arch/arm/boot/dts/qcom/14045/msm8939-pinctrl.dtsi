#include "../msm8939-pinctrl.dtsi"

&soc {
	tlmm_pinmux: pinctrl@1000000 {
		pmx_i2c_6 {
			qcom,pins = <&gp 22>, <&gp 23>;
			qcom,num-grp-pins = <1>;
		};
		
		sim2_cd_pin {
			qcom,pins = <&gp 56>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "sim2-cd-gpio";
			
			sim2_cd_on: cd_on {
				drive-strength = <2>;
				bias-pull-up;
			};
			
			sim2_cd_off: cd_off {
				drive-strength = <2>;
				bias-disable;
			};
		};
			
		sim1_cd_pin {
			qcom,pins = <&gp 60>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "sim1-cd-gpio";
			
			sim1_cd_on: cd_on {
				drive-strength = <2>;
				bias-pull-up;
			};
			
			sim1_cd_off: cd_off {
				drive-strength = <2>;
				bias-disable;
			};
		};
		
		sdc {
			qcom,pin-type-sdc;
		};
		
		pmx_i2c_1 {
			/* CLK, DATA */
			qcom,pins = <&gp 2>, <&gp 3>;
			qcom,num-grp-pins = <2>;
			qcom,pin-func = <3>;
			label = "pmx_i2c_1";
		
			i2c_1_active: i2c_1_active {
				drive-strength = <2>; /* 2 MA */
				bias-disable;		  /* No PULL */
			};
		
			i2c_1_sleep: i2c_1_sleep {
				drive-strength = <2>; /* 2 MA */
				bias-disable;		  /* No PULL */
			};
		};
		
		pmx_i2c_3 {
			/* CLK, DATA */
			qcom,pins = <&gp 11>, <&gp 10>;
			qcom,num-grp-pins = <2>;
			qcom,pin-func = <2>;
			label = "pmx_i2c_3";

			i2c_3_active: i2c_3_active {
				drive-strength = <2>; /* 2 MA */
				bias-disable;         /* No PULL */
			};

			i2c_3_sleep: i2c_3_sleep {
				drive-strength = <2>; /* 2 MA */
				bias-disable;	      /* No PULL */
			};
		};	
		
		usb-id-pin {
			qcom,pins = <&gp 98>;
			
			usbid_sleep: sleep {
				drive-strength = <8>; 
				bias-disable = <0>;	 
			};
		};

		charging_switch1 {
			qcom,pins = <&gp 23>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "charging-switch1";

			charging_switch1_active: active {
				drive-strength = <2>;
				bias-pull-up;
			};

			charging_switch1_sleep: sleep {
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		charging_switch2 {
			qcom,pins = <&gp 113>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "charging-switch2";

			charging_switch2_active: active {
				drive-strength = <2>;
				bias-pull-up;
			};

			charging_switch2_sleep: sleep {
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		charging_switch3 {
			qcom,pins = <&gp 38>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "charging-switch2";

			charging_switch3_active: active {
				drive-strength = <2>;
				bias-pull-up;
			};

			charging_switch3_sleep: sleep {
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		charging_reset {
			qcom,pins = <&gp 22>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "charging-reset";

			charging_reset_active: active {
				drive-strength = <2>;
				bias-pull-up;
			};

			charging_reset_sleep: sleep {
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		charging_clock {
			qcom,pins = <&gp 115>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "charging-clock";

			charging_clock_active: active {
				drive-strength = <2>;
				bias-pull-up;
			};

			charging_clock_sleep: sleep {
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		charging_data {
			qcom,pins = <&gp 109>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "charging-data";

			charging_data_active: active {
				drive-strength = <2>;
				bias-disable;
			};

			charging_data_sleep: sleep {
				drive-strength = <2>;
				bias-disable;
			};
		};

		spi0_active {
			qcom,pins = <&gp 9>, <&gp 11>;
            		qcom,num-grp-pins = <2>;
		};
		
		spi0_suspend {
			qcom,pins =  <&gp 9>, <&gp 11>;
			qcom,num-grp-pins = <2>;
		};
		
		tlmm_gpio_key {
			qcom,pins = <&gp 107>, <&gp 108>;
			qcom,num-grp-pins = <2>;
		};
		
		apds99xx_int_pin {
			qcom,pins = <&gp 9>;
		};

		ts_id1 {
			qcom,pins = <&gp 51>;
			qcom,pin-func = <0>;
			qcom,num-grp-pins = <1>;
			label = "ts_id1";

			ts_id1_default: ts_id1_default {
				drive-strength = <2>;
				bias-pull-down;
			};
		
		};

		cdc-ext-pa-lines {
			qcom,pins = <&gp 113>, <&gp 114>,
					<&gp 115>;
			qcom,num-grp-pins = <3>;
			qcom,pin-func = <1>;

			label = "cdc-ext-pa-lines";
			
			cdc_ext_pa_act: ext_pa_on {
				drive-strength = <8>;
				bias-pull-none;
			};

			cdc_ext_pa_sus: ext_pa_off {
				drive-strength = <2>;
				bias-disable;
			};
		};
		
		cdc-ext-pa-ws-line {
			qcom,pins = <&gp 110>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <2>;
			
			label = "cdc-ext-pa-ws-line";
			
			cdc_ext_pa_ws_act: ext_pa_on {
				drive-strength = <8>;
				bias-pull-none;
			};

			cdc_ext_pa_ws_sus: ext_pa_off {
				drive-strength = <2>;

				bias-disable;
			};
		};
		
		mclk-lines {
			qcom,pins = <&gp 116>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <2>;
			label = "mclk-lines";
			mclk_lines_act: mclk_lines_on {
				drive-strength = <8>;
			};
			mclk_lines_sus: mclk_lines_off {
				drive-strength = <2>;
				bias-disable;
			};
		};	

		lis3dh_int1_pin {
			qcom,pins = <&gp 115>;
			qcom,num-grp-pins = <1>;
			label = "lis3dh_int_pin";

			lis3dh_int1_default: int1_default {
				drive-strength = <6>;
				bias-pull-down;
			};

			lis3dh_int1_sleep: int1_sleep {
				drive-strength = <2>;
				bias-pull-down;
			};
		};
		
		ext-cdc-tlmm-lines {
			tlmm_lines_on {
				bias-pull-none;
			};
		};
		
		audio-vdd-enable {
			qcom,pins = <&gp 109>;
			qcom,num-grp-pins = <1>;
			
			qcom,pin-func = <0>;
			label = "audio-vdd-en";
			
			audio_vdd_enable_act: audio_vdd_on {
				drive-strength = <16>;
				bias-pull-none;

			};

			audio_vdd_enable_sus: audio_vdd_off {
				drive-strength = <2>;
				bias-disable;
			};
		};
		
		sdhc2_vdd_pin {
			qcom,pins = <&gp 121>;
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "vdd-gpio-en";
		};
		
		pmx_mdss {
			active {
				output-high;
			};

			suspend {
				output-low;
			};
		};
	};
};
